<p id="notice"><%= notice %></p>

<h1>Notes</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Instrument</th>
      <th>Timestamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <% @notes.each do |note| %>
        <% render_partial :note %>
      <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
